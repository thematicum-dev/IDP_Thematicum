{
  "name": "thematicum",
  "version": "1.0.0",
  "private": true,
  "scripts": {
	"start": "babel-node ./bin/www",
	"dev": "nodemon ./bin/www --config nodemon.json --exec babel-node --inspect",
  "build": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
	"build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.shim.ts'"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.1",
    "@angular/upgrade": "2.0.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.15.2",
    "bower": "^1.7.9",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "~4.14.0",
    "hbs": "~3.1.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.5",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "~1.6.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "underscore": "^1.8.3",
    "validator": "^6.1.0",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/core-js": "0.9.35",
    "@types/jquery": "^2.0.39",
    "@types/node": "^6.0.45",
    "angular2-router-loader": "^0.3.2",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^3.1.2",
    "del-cli": "^0.2.0",
    "html-loader": "^0.4.4",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "typescript": "^2.0.3",
    "webpack": "2.2.1",
    "webpack-merge": "^0.14.1"
  }
}
